# include <stdio.h>
int main()
{
int x;
scanf("%d",&x);
switch(x)
case 1: prntf("sunday");
case 2: printf("monday");
case 3: prntf("tuesday");
case 4: printf("wednesday");
case 5: printf("thursday");
case 6: printf("friday");
case 7: printf("saturday");
}
return0;
}






































































































